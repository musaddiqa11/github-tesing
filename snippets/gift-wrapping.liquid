                {% if block.settings.paid_gift_title != blank %}<h4>{{block.settings.paid_gift_title}}</h4>{% endif %}


            <input  class="gift-wrapping__check hidden"
                id="gift-wrapping"
                type="checkbox"
                  name="attributes[paid-gift-wrapping]" value="yes" {% if
                cart.attributes.paid-gift-wrapping %} checked="checked" {% endif %} style="float: none" />
                <label for="gift-wrapping">
                    {% assign paid_gift_price = product.selected_or_first_available_variant.price | money %}
                    {{block.settings.paid_gift_text | replace: "<price>", paid_gift_price}}
                </label>
                <button 
                class="button button--small button--secondary gift-wrapping__button {% if cart.attributes.paid-gift-wrapping %} gift-wrapping__button--selected{% endif %}" 
                data-input="gift-wrapping" 
                data-note="gift-note">
          {% if cart.attributes.paid-gift-wrapping %}{{block.settings.paid_gift_button_remove}}{% else %}{{block.settings.paid_gift_button}}{% endif %}
        </button>
        {% if cart.attributes.paid-gift-wrapping != blank %}
        <div class="gift-wrap__bottom">
          <label for="gift-note">Add a note to your order</label>
          <textarea 
                    name="attributes[gift-note]" 
                    id="gift-note" 
                    {% unless cart.attributes.paid-gift-wrapping %} disabled {% endunless %}>{{- cart.attributes.gift-note -}}</textarea>
        </div>
        {% endif %}

        </div>
            {%- endif -%}
    </div>
    </div>

  {% assign id = product.selected_or_first_available_variant.id %}

    {% assign gift_wraps_in_cart = 0 %}
    {% for item in cart.items %}
    {% if item.id == id %}
    {% assign gift_wraps_in_cart = item.quantity %}
    {% endif %}
    {% endfor %}

    <style>
    .cart__qty{{id}} {display: none;}
    

gift-note.cart__note .gift-wrapping__wrapper {
    display: flex;
  }
  
  gift-note.cart__note .gift-wrapping__wrapper .gift-wrapping__container {
    flex: 1 1 auto;
    width: 100%;
    text-align: center;
    padding: 30px 20px 20px;
    border: 5px solid #eee;
    color: var(--color-text);
  }
  
  gift-note.cart__note .gift-wrapping__wrapper .gift-wrapping__container .imgWrap img {
    width: 84px;
    height: auto;
    display: block;
    margin: auto;
    margin-bottom: 14px;
  }
  
  gift-note.cart__note .gift-wrapping__wrapper .gift-wrapping__container button.gift-wrapping__button {
    margin-top: 12px;
    margin-bottom: 12px;
    line-height: 20px;
    height: 40px;
    min-height: 0;
    min-width: 0;
    padding: 5px 13px 5px;
  }
  
  gift-note.cart__note .gift-wrapping__wrapper .gift-wrapping__container textarea {
    width: 100%;
    outline: none;
    box-shadow: none;
    margin: 0;
    padding: 10px;
    min-height: 52px;
  }
  
  gift-note.cart__note .gift-wrapping__wrapper .gift-wrapping__container label {
    display: block;
    position: static;
    line-height: normal;
    height: auto;
  }
  
  gift-note.cart__note {
    max-width: 50%;
    width: 50rem;
  }
  
  gift-note.cart__note:after {
    border: 0 none;
    box-shadow: none;
  }
  
  gift-note.cart__note .gift-wrapping__wrapper .gift-wrapping__container .imgWrap {
    width: 80px;
    margin: auto;
  }
  gift-note.cart__note h3{
    margin-top: 0;
  }
  .cart__footer .cart__note.field{
    margin-top: 42px;
  }
  @media (max-width: 749px) {
    gift-note.cart__note .gift-wrapping__wrapper {
      display: flex;
      flex-wrap: wrap;
    }
  
    gift-note.cart__note .gift-wrapping__wrapper .gift-wrapping__container {
      flex: 0 0 100%;
      width: 100%;
    }
  }
  
  
  /*================ Spinner Icon ================*/
  gift-note.cart__note .icon {
    display: inline-block;
    width: 20px;
    height: 20px;
    vertical-align: middle;
    fill: currentColor;
  }
  gift-note.cart__note .icon-spinner {
    -moz-animation: spin 500ms infinite linear;
    -o-animation: spin 500ms infinite linear;
    -webkit-animation: spin 500ms infinite linear;
    animation: spin 500ms infinite linear; }
  
    @-webkit-keyframes spin {
      0% {
        -ms-transform: rotate(0deg);
        -webkit-transform: rotate(0deg);
        transform: rotate(0deg); }
      100% {
        -ms-transform: rotate(360deg);
        -webkit-transform: rotate(360deg);
        transform: rotate(360deg); } }
    
    @-moz-keyframes spin {
      0% {
        -ms-transform: rotate(0deg);
        -webkit-transform: rotate(0deg);
        transform: rotate(0deg); }
      100% {
        -ms-transform: rotate(360deg);
        -webkit-transform: rotate(360deg);
        transform: rotate(360deg); } }
    
    @-ms-keyframes spin {
      0% {
        -ms-transform: rotate(0deg);
        -webkit-transform: rotate(0deg);
        transform: rotate(0deg); }
      100% {
        -ms-transform: rotate(360deg);
        -webkit-transform: rotate(360deg);
        transform: rotate(360deg); } }
    
    @keyframes spin {
      0% {
        -ms-transform: rotate(0deg);
        -webkit-transform: rotate(0deg);
        transform: rotate(0deg); }
      100% {
        -ms-transform: rotate(360deg);
        -webkit-transform: rotate(360deg);
        transform: rotate(360deg); } }
    </style>
    <script>
      Shopify.Cart = Shopify.Cart || {};

      Shopify.Cart.GiftWrap = {};
  
      Shopify.Cart.GiftWrap.set = function () {
          var headers = new Headers({ 'Content-Type': 'application/json' });
          var request = {
                  method: 'POST',
                  headers: headers,
                  body: JSON.stringify({ 
                      updates: { {{ id }}: 1 }, attributes: { 'paid-gift-wrapping': true, 'gift-note': null, 'free-gift-wrapping': null, 'free-gift-note': null  } 
                  })
              };
          fetch('/cart/update.js', request).then(function () { location.href = '/cart'; });
      }
  
      Shopify.Cart.GiftWrap.remove = function () {
          var headers = new Headers({ 'Content-Type': 'application/json' });
  
          var request = {
                  method: 'POST',
                  headers: headers,
                  body: JSON.stringify({ 
                      updates: { {{ id }}: 0}, attributes: { 'paid-gift-wrapping': null, 'gift-note': null, 'free-gift-note': null  } 
                  })
              };
              fetch('/cart/update.js', request).then(function () { location.href = '/cart'; });
      }
  
  
      // Free gift message
      Shopify.Cart.freeGiftWrap = {};
  
      Shopify.Cart.freeGiftWrap.set = function () {
          var headers = new Headers({ 'Content-Type': 'application/json' });
          var request = {
                  method: 'POST',
                  headers: headers,
                  body: JSON.stringify({ 
                      // This is a free message so don't needed to add product
                      updates: {  {{ id }}: 0 }, attributes: { 'free-gift-wrapping': true, 'free-gift-note': null, 'paid-gift-wrapping': null, 'gift-note': null } 
                  })
              };
          fetch('/cart/update.js', request).then(function () { location.href = '/cart'; });
      }
  
      Shopify.Cart.freeGiftWrap.remove = function () {
          var headers = new Headers({ 'Content-Type': 'application/json' });
  
          var request = {
                  method: 'POST',
                  headers: headers,
                  body: JSON.stringify({ 
                      // This is a free message so don't needed to add product
                      updates: { /* {{ id }}: 0 */ }, attributes: { 'free-gift-wrapping': null, 'free-gift-note': null, 'gift-note': null } 
                  })
              };
              fetch('/cart/update.js', request).then(function () { location.href = '/cart'; });
      }
  
      // If we have nothing but gift-wrap items in the cart.
      {% if cart.items.size == 1 and gift_wraps_in_cart > 0 %}
      document.addEventListener("DOMContentLoaded", function () {
          Shopify.Cart.GiftWrap.remove();
      });
      // If we have more than one gift-wrap item in the cart.
      {% elsif gift_wraps_in_cart > 1 %}
      document.addEventListener("DOMContentLoaded", function () {
          Shopify.Cart.GiftWrap.set();
      });
      // If we have a gift-wrap item in the cart but our gift-wrapping cart attribute has not been set.
      {% elsif gift_wraps_in_cart > 0 and cart.attributes.paid-gift-wrapping == blank %}
      document.addEventListener("DOMContentLoaded", function () {
          Shopify.Cart.GiftWrap.set();
      });
      // If we have no gift-wrap item in the cart but our gift-wrapping cart attribute has been set.
      {% elsif gift_wraps_in_cart == 0 and cart.attributes.paid-gift-wrapping != blank %}
      document.addEventListener("DOMContentLoaded", function () {
          Shopify.Cart.GiftWrap.set();
      });
      {% endif %}
  
      //when clicking the gift button, click on the checkbox for that gift option
      let giftButtons = document.querySelectorAll('.gift-wrapping__button');
      giftButtons.forEach(function(button){
          var input = button.dataset.input;
          let loader = '<span data-loader><svg aria-hidden="true" focusable="false" role="presentation" class="icon icon-spinner" viewBox="0 0 20 20"><path d="M7.229 1.173a9.25 9.25 0 1 0 11.655 11.412 1.25 1.25 0 1 0-2.4-.698 6.75 6.75 0 1 1-8.506-8.329 1.25 1.25 0 1 0-.75-2.385z" fill="#919EAB"/></svg></span>';
          
          //console.log(input);
          button.addEventListener('click',function(e){
              e.preventDefault();
              button.innerHTML = loader;
              let inputEl = document.getElementById(input); 
              //console.log(input,inputEl);
              inputEl.removeAttribute('disabled')
              inputEl.click();
          });
      });
      
      // When the Paid gift-wrapping checkbox is checked or unchecked.
      document.addEventListener("DOMContentLoaded", function () {
          {% if block.settings.paid_gift_enabled %}
          document.querySelector('[name="attributes[paid-gift-wrapping]"]').addEventListener("change", function (event) {
              if (event.target.checked) {
                  Shopify.Cart.GiftWrap.set();
              } else {
                  Shopify.Cart.GiftWrap.remove();
              }
          });
         
          {% if cart.attributes.paid-gift-wrapping != blank %}
            document.querySelector('#gift-note').addEventListener("change", function (evt) {
                var note = evt.target.value;
                var headers = new Headers({ 'Content-Type': 'application/json' });
                var paidgiftCheckBox = document.getElementById("gift-wrapping");
                
                var request = {
                    method: 'POST',
                    headers: headers,
                    body: JSON.stringify({ attributes: { 'gift-note': note } })
                };
                if (paidgiftCheckBox.checked == true){
                    fetch('/cart/update.js', request);
                }
            });
          {% endif %}
          {%endif %}
      });
  
