            {% endif %}
            {% if block.settings.free_gift_title != blank %}<h4>{{block.settings.free_gift_title}}</h4>{% endif %}

                <input
                class="gift-wrapping__check hidden" 
                id="free-gift-wrapping"
                type="checkbox"
                name="attributes[free-gift-wrapping]" 
                value="yes" 
                {% if cart.attributes.free-gift-wrapping %} checked="checked" {% endif %}
                />
                <label for="free-gift-wrapping">{{block.settings.free_gift_text}}</label>
                <button 
                    type="button"
                    class="button button--small button--secondary gift-wrapping__button {% if cart.attributes.free-gift-wrapping != blank %} gift-wrapping__button--selected{% endif %}" 
                    data-input="free-gift-wrapping" 
                    data-note="free-gift-note">
                    {% if cart.attributes.free-gift-wrapping != blank  %}{{block.settings.free_gift_button_remove}}{% else %}{{block.settings.free_gift_button}}{% endif %}
                </button>
                {% if cart.attributes.free-gift-wrapping %}
                <div class="gift-wrap__bottom">
                    <label for="free-gift-note">Gift Message (up to 100 characters):</label>
                    <textarea name="attributes[free-gift-note]" id="free-gift-note">{{ cart.attributes.free-gift-note }}</textarea>
                </div>
                {%- endif -%}
            </div>
            {%- endif -%}
            
            
